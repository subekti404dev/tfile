{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pnpm run build:clean && pnpm run build:js && pnpm run build:cjs",
    "build:clean": "rimraf dist/",
    "build:js": "npx tsc",
    "build:cjs": "npx esbuild dist/index.js  --bundle --outfile=dist/app.cjs --format=cjs --platform=node && ncc build dist/app.cjs -o single",
    "build:bin": "cd single && bun build ./index.cjs --compile --outfile ./bin && cd ..",
    "start": "node dist/index.js",
    "dev": "pnpm run build && concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mime-types": "^2.1.35",
    "multer": "1.4.5-lts.1",
    "nodejs-base64": "^2.0.0",
    "pocketbase": "^0.19.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.201",
    "@types/md5": "^2.3.5",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.11.9",
    "@types/socket.io": "^3.0.2",
    "@vercel/ncc": "^0.38.1",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.5",
    "typescript": "^4.8.4"
  }
}
